<%= simple_form_for(@saving, html: { class: 'text-white' }) do |f| %>
  <%= f.input :name, input_html: { class: 'form-control dark-form-input'} %>
  <%= f.input :amount, input_html: { class: 'form-control dark-form-input'} %>
  <%= f.submit "Submit Savings", class: "btn btn-primary" %>
  <%= f.submit "Submit savings", class: "btn btn-primary" %>
<% end %>