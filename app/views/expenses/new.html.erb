<%= simple_form_for(@expense, html: { class: 'text-white' }) do |f| %>
  <%= f.input :name, input_html: { class: 'form-control dark-form-input'} %>
  <%= f.input :amount, input_html: { class: 'form-control dark-form-input'} %>
  <%= f.input :category, input_html: { class: 'form-control dark-form-input'} %>
  <%= f.range_field :amount, :min=>0, :max=>200, :class=>"slide", :id=>"slider1"%>
  <%= f.submit "Submit Expenses", class: "btn btn-primary" %>
<% end %>
